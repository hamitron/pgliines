<div class="tab-content">
  <div class="tab-pane active" id="plus">       
      <a href="#new" role="tab" data-toggle="tab"><div class="new-milestone"></div></a>
  </div>
        
        <div class="tab-pane " id="new">
             
                <%= form_for @twiine do |s| %>
                    <div class="form-group">
                        <label for="name">Name</label>
                            <%= s.text_field :name, class: "form-control",  placeholder: "twiine name" %> 
                    </div>

                    <div class="form-group">
                        <label for="description">Description</label>
                            <%= s.text_field :category, class: "form-control", placeholder: "category"  %> 
                    </div>

                    <div class="form-group">
                            <%= s.submit "Create Twiine", class: "btn btn-default" %>
                            <%= s.hidden_field :user_id %>
                    </div>
                <% end %>
        
        </div>